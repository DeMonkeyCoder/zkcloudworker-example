{
  "name": "worker-example",
  "version": "0.1.2",
  "author": "DFST",
  "scripts": {
    "test": "jest",
    "local": "jest --chain=local --deploy=true --send=false --one=false --many=true",
    "lightnet.deploy": "jest --chain=lightnet --deploy=true --one=false --many=false",
    "lightnet.run": "jest --chain=lightnet --deploy=false --one=true --many=true",
    "zeko.deploy": "jest --chain=zeko --deploy=true --cloud=local --one=false --many=false",
    "zeko.run": "jest --chain=zeko --deploy=false --cloud=local --one=false --many=true",
    "devnet.deploy": "jest --chain=devnet --deploy=true --cloud=local --one=false --many=false",
    "devnet.run": "jest --chain=devnet --deploy=false --one=true --many=false --send=false"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.2.1",
  "dependencies": {
    "o1js": "^1.1.0",
    "zkcloudworker": "^0.5.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "slowTestThreshold": 1500,
    "testTimeout": 10800000,
    "bail": true,
    "verbose": true,
    "roots": [
      "tests"
    ],
    "setupFilesAfterEnv": [
      "./jest-config.ts"
    ]
  }
}
